{
    "name": "carlosv2/funnel",
    "type": "library",
    "description": "Little testing repositories enhancer",
    "keywords": [
        "Repository",
        "Filesystem",
        "Test",
        "PersistedObjects"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Ortega",
            "email": "carlosV2.0@gmail.com"
        }
    ],
    "autoload": {
        "files": ["src/bootstrap.php"],
        "psr-4": {
            "carlosV2\\Funnel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\carlosV2\\Funnel\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:HummingbirdTechGroup/Communism.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:HummingbirdTechGroup/persisted-objects.git"
        }
    ],
    "require": {
        "php": ">5.3",
        "everzet/persisted-objects": "dev-hb-fork",
        "carlosv2/communism": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
